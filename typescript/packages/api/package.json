{
  "name": "@songbird/api",
  "packageManager": "yarn@3.3.0",
  "private": true,
  "dependencies": {
    "@songbird/precedent-iso": "workspace:*",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -b -w",
    "run-server::prod": "node dist/index.js",
    "run-server::develop": "node --watch dist/index.js",
    "test": "yarn vitest src/tests",
    "lint": "eslint . --ext .ts",
    "fix": "yarn fix::lint && yarn fix::format",
    "fix::lint": "eslint . --ext .ts --fix",
    "fix::format": "prettier --write .",
    "check": "yarn check::lint && yarn check::format",
    "check::lint": "eslint . --ext .ts",
    "check::format": "prettier --check ."
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/node": "^18.11.12",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "prettier": "^2.8.1",
    "vitest": "^0.25.6"
  }
}
