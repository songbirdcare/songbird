{
  "name": "@songbird/app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev::server": "NODE_OPTIONS='--inspect' next dev",
    "dev::watch": "yarn tsc --noEmit --incremental --watch",
    "dev": "concurrently --names \"NEXT,WATCH\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn dev::server\" \"yarn dev::watch\"",
    "build": "next build",
    "start": "next start",
    "test": "yarn vitest src/tests",
    "fix": "yarn fix::lint; yarn fix::format",
    "fix::lint": "eslint . --fix",
    "fix::format": "prettier --write pages/",
    "check": "yarn check::lint && yarn check::format",
    "check::lint": "eslint . --ext .ts",
    "check::format": "prettier --check .",
    "clean": "yarn clean::dist",
    "clean::dist": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^1.8.0",
    "@auth0/nextjs-auth0": "^2.2.1",
    "@datadog/browser-rum": "^4.34.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@formsort/react-embed": "^3.1.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "@mui/x-data-grid": "^5.17.24",
    "@next/font": "^13.1.6",
    "@songbird/precedent-iso": "workspace:^",
    "flagsmith": "^3.16.0",
    "logrocket": "^3.0.1",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-calendly": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-use-intercom": "^4.0.0",
    "swr": "^2.0.3",
    "typescript-cookie": "^1.0.4",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.6",
    "@types/eslint": "^8.21.1",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
